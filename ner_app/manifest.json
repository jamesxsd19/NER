{
    "manifest_version": 3,
    "name": "NER APP",
    "version": "1.0",
    "description": "Scan articles for important entities using NER model",
    "action": {
      "default_popup": "scripts/popup.html"
    },
    "content_scripts": [
      {
        "js": [
          "scripts/content.js"
        ],
        "matches": [
          "https://developer.chrome.com/docs/extensions/*",
          "https://developer.chrome.com/docs/webstore/*",
          "https://text.npr.org/*",
          "https://lite.cnn.com/en/article/*"
        ]
      }
    ]
  }